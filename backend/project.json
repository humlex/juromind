{
  "name": "backend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run python manage.py runserver"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry run python manage.py migrate"
      }
    }
  }
}
